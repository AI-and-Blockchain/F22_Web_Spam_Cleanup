
{
  "manifest_version": 3,
  "name": "Clean My Web",
  "version": "1.0",
  "description": "Show crowdsourced metrics next to google searches",
  "icons": {
      "16": "icon-16.png",
      "32": "icon-32.png",
      "48": "icon-48.png",
      "128": "icon-128.png"
  },
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab", 
    "scripting",
    "contextMenus",
    "storage"
  ]
}